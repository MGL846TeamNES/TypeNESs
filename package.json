    "shelljs": "^0.7.7",
    "ts-jest": "^22.4.1",
    "ts-node": "^4.0.2",
    "tslint": "^5.8.0",
    "web-audio-mock": "^0.1.9",
    "canvas": "^1.6.5",
    "webpack": "^3.10.0"
  },
  "dependencies": {
    "npm": "^5.7.1"
  }
